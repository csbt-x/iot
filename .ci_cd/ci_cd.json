{
	"push": {
		"feature/workflows": {
			"distribute": {
				"tags": "develop,$version"
			},
			"deploy": {				
				"environment": "staging"
			}
		},
    "master": {
      "distribute": {
				"tags": "latest"
			}
    }
	},
	"release": {
		"distribute": {
			"tags": "latest,$version"
		},
		"deploy": {
			"managerTag": "latest",
			"environment": "production"
		}
	},
	"managerDockerPush": {		
		"develop": {
			"deploy": {
				"environment": "staging"
			}
		}
	}
}
