{
  "name": "@openremote/manager",
  "version": "1.0.4",
  "description": "OpenRemote Manager",
  "author": "OpenRemote",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "exports": {
    "./*": "./lib/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/*"
      ]
    }
  },
  "scripts": {
    "clean": "npx tsc -b --clean && npx shx rm -rf dist",
    "modelBuild": "npx orutil build",
    "modelWatch": "npx orutil watch",
    "build": "npx cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack --mode production",
    "serve": "npx tsc -b --clean && npx shx rm -rf dist && npx orutil build && npx cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack serve --mode development",
    "serveNoModelBuild": "npx tsc -b --clean && npx cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack serve --mode development",
    "test": "cucumber-js --config test/cucumber.js ./test/features/*.feature --tags 'not @separate'",
    "testTagged": "cucumber-js --config test/cucumber.js ./test/features/*.feature --tags",
    "testSeparated":"cucumber-js --config test/cucumber.js ./test/features/*.feature --tags @separate"
  },
  "dependencies": {
    "@openremote/or-app": "^1.0.4"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^8.2.2",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "@openremote/util": "workspace:*",
    "@playwright/test": "^1.19.2",
    "cucumber-pretty": "^1.5.2",
    "dotenv": "^16.0.0",
    "playwright": "^1.19.1",
    "playwright-expect": "^0.1.2"
  }
}
