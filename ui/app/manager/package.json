{
  "name": "@openremote/manager",
  "version": "1.0.4",
  "description": "OpenRemote Manager",
  "author": "OpenRemote",
  "license": "AGPL-3.0-or-later",
  "private": true,
  "exports": {
    "./*": "./lib/*.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "lib/*"
      ]
    }
  },
  "scripts": {
    "clean": "npx tsc -b --clean && npx shx rm -rf dist",
    "modelBuild": "npx orutil build",
    "modelWatch": "npx orutil watch",
    "build": "npx cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack --mode production",
    "serve": "npx orutil build && npx cross-env NODE_OPTIONS=--max_old_space_size=4096 webpack serve --mode development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@openremote/or-app": "^1.0.4"
  },
  "devDependencies": {
    "@openremote/util": "^1.0.4",
    "ts-loader": "^9.2.6",
    "typescript": ">=4.5.2",
    "webpack": "^5.64.3",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  }
}
