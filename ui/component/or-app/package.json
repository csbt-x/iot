    "@openremote/or-map": "^1.0.4",
    "@openremote/or-rules": "^1.0.4",
    "@openremote/or-smart-notify": "^1.0.3",
    "@openremote/or-timeline": "^1.0.3",
    "@openremote/rest": "^1.0.3",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/navigo": "^7.0.1",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "lit": "^2.0.0-rc.2",
